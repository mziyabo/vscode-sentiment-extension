{
  "name": "vscode-sentiment",
  "version": "0.0.1",
  "description": "vscode sentiment analysis",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/node": "^12.7.3",
    "@types/vscode": "^1.37.0",
    "aws-sdk": "^2.521.0",
    "sentiment": "^5.0.2",
    "typescript": "^3.6.2",
    "vscode": "^1.1.36"
  },
  "activationEvents": [
    "onCommand:extension.analyze",
    "onLanguage:markdown"
  ],
  "engines": {
    "vscode": "^1.22.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.analyze",
        "title": "Perform sentiment analysis"
      }
    ],
    "colors": [
      {
        "id": "codesentiment.NegativeSentiment",
        "description": "Background decoration color for large numbers",
        "defaults": {
          "dark": "#FF000055",
          "light": "#FF000055",
          "highContrast": "#FF000055"
        }
      },
      {
        "id": "codesentiment.PositiveSentiment",
        "description": "Background decoration color for large numbers",
        "defaults": {
          "dark": "#73ff002f",
          "light": "#73ff002f",
          "highContrast": "#73ff002f"
        }
      }
    ]
  }
}